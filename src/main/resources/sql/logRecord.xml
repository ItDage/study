<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.study.dao.LogDao">

	<insert id="add" parameterType="com.study.entity.LogRecord">
		insert into logrecord(mail, scan_time, url) values(#{mail}, #{scan_time}, #{url})
	</insert>
	
</mapper>